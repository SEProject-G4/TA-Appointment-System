{
  "dependencies": {
    "express-session": "^1.17.3",
    "connect-mongo": "^5.1.0",
    "node-cache": "^5.1.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "db:indexes": "node -e 'require(\"./src/utils/database-optimization\").createIndexes().then(() => process.exit(0))'",
    "db:maintenance": "node -e 'require(\"./src/utils/database-optimization\").performMaintenance().then(() => process.exit(0))'",
    "db:cleanup": "node -e 'require(\"./src/utils/database-optimization\").cleanupSessions().then(() => process.exit(0))'"
  }
}